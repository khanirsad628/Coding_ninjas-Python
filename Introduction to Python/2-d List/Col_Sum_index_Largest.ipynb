    "def lar(li):\n",
    "    l = []\n",
    "    for j in range(len(li[0])):\n",
    "        sum = 0\n",
    "        for i in range(len(li)):\n",
    "            sum = sum+li[i][j]\n",
    "        l.append(sum)\n",
    "    return l.index(max(l))\n",
    "            \n",
    "\n",
    "l = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]\n",
    "print(lar(l))"
   ]
