    "# Reversed Pattern"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "draw reversed Pattern<br>\n",
    "for n = 4<br>\n",
    "   *    --> total spaces 3\n",
    "  **    --> total spaces 2\n",
    " ***    -->total spaces 1\n",
    "****    -->total spaces 0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n",
      "    *\n",
      "   **\n",
      "  ***\n",
      " ****\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "i = 1\n",
    "k = n\n",
    "while(i<=n):\n",
    "    s = 0\n",
    "    while(s<k):\n",
    "        print(' ',end='')\n",
    "        s = s+1\n",
    "    j = s\n",
    "    while(j<=n):\n",
    "        print('*',end='')\n",
    "        j = j+1\n",
    "    k = k-1\n",
    "    print()\n",
    "    i = i+1\n",
    "    "
   ]
  },
