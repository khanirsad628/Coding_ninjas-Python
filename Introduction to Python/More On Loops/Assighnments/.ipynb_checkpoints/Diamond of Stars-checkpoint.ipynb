   "source": [
    "n = int(input())\n",
    "i = 1\n",
    "fh = (n+1)//2\n",
    "while(i<=fh):\n",
    "    spaces = fh-i\n",
    "    while(spaces>0):\n",
    "        print(\" \",end='')\n",
    "        spaces = spaces+1\n",
    "    j = (2*i)-1\n",
    "    while(j>0):\n",
    "        print(\"*\",end='')\n",
    "        j = j-1\n",
    "    print()\n",
    "    i = i+1\n",
    "    "
