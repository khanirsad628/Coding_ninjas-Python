   "source": [
    "n = int(input())\n",
    "r = int(input())\n",
    "def fact(n):\n",
    "    if n==1:\n",
    "        return 1 \n",
    "    else:\n",
    "        return n*fact(n-1)\n",
    "    \n",
    "ncr = fact(n)//(fact(r)*(fact(n-r)))\n",
    "print(ncr)"
